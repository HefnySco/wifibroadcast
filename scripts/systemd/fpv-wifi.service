
[Unit]
Description=FPV WIFI Broadcast
After=network-online.target

[Service]

## Pipeline for PI camera
ExecStart=/bin/sh -c "/home/pi/wfb-ng/scripts/tx_standalone.sh wlan1 5"

##

Type=simple
Restart=always
RestartSec=1s
TimeoutStopSec=10s
KillMode=control-group

[Install]
WantedBy=multi-user.target

